docker image build \
-t imagename \
--build-arg DB_NAME=edmv4 \
--build-arg DB_SERVER=database\
--build-arg DB_USER=postgres \
--build-arg DB_PASSWORD=123456789 \
--build-arg DB_PORT=5432 \
--build-arg ADMIN_PASS=12345 \
--build-arg XMS=256m \
--build-arg XMX=512m \
--build-arg MAX_PERM=192M \
--no-cache .